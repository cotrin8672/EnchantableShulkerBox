{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "${modAuthors}"
  ],
  "contact": {
    "sources": "https://github.com/"
  },
  "license": "${modLicense}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.cotrin8672.enchantableshulkerbox.fabric.EnchantableShulkerBoxFabric"
      }
    ]
  },
  "mixins": [
    "${modId}.mixins.json",
    "${modId}-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricVersion}",
    "minecraft": "~${minecraftVersion}",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
